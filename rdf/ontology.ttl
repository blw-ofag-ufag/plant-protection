    rdfs:domain <https://schema.org/Organization> ;
    rdfs:isDefinedBy <https://www.rfc-editor.org/rfc/rfc3966> ;
    rdfs:range xsd:string .

<https://schema.org/legalName> a owl:DatatypeProperty ;
