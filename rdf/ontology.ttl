@prefix chebi: <http://purl.obolibrary.org/obo/CHEBI_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix srppp: <https://agriculture.ld.admin.ch/plant-protection/> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix wdt: <http://www.wikidata.org/prop/direct/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

chebi:24431 a owl:Class ;
    rdfs:label "Chemische Entität"@de,
        "chemical entity"@en ;
    rdfs:comment "Eine chemische Entität, wie in der CheBI-Ontologie definiert, dient als Referenzpunkt zur Identifizierung und Klassifizierung von Substanzen."@de,
        "A chemical entity as defined in the CheBI ontology, serving as a reference point for identifying and classifying substances."@en .

wd:Q16521 a owl:Class ;
    rdfs:label "Biologisches Taxon"@de,
        "Biological taxon"@en ;
    rdfs:comment "Gruppe von einem oder mehreren Organismen, die ein Taxonom als Einheit betrachtet."@de,
        "Group of one or more organism(s), which a taxonomist adjudges to be a unit."@en .

srppp:AbioticStressor a owl:Class ;
    rdfs:label "Abiotischer Schadverursacher"@de,
        "Abiotic crop stressor"@en ;
    rdfs:comment "Negative Auswirkungen nicht lebender Faktoren auf die Kulturpflanze in einer bestimmten Umgebung."@de,
        "Negative impact of non-living factors on the crop in a specific environment."@en ;
    rdfs:isDefinedBy wd:Q4667893 ;
    rdfs:seeAlso <https://w.wiki/CJnE> ;
    rdfs:subClassOf srppp:CropStressor,
        srppp:EnvironmentalCondition ;
    owl:disjointWith srppp:BioticStressor .

srppp:Acaricide a owl:Class ;
    rdfs:label "Akarizid"@de,
        "Acaricide"@en ;
    rdfs:comment "Ein Akarizid ist ein chemisches Pflanzenschutzmittel, das zur Bekämpfung von Milben und Zecken eingesetzt wird, die häufig Schäden an landwirtschaftlichen Kulturen verursachen."@de,
        "An acaricide is a chemical crop protection product used to control mites and ticks, which often cause damage to agricultural crops."@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:Action a owl:Class ;
    rdfs:label "Schutzmassnahme"@de,
        "Protective action"@en ;
    rdfs:comment "Eine spezifische Massnahme zur Minderung von Schäden an einer landwirtschaftlichen Kultur."@de,
        "A specific action taken to mitigate damage done to crops."@en ;
    skos:scopeNote "Diese Klasse ist nur dann zu verwenden, wenn es sich bei der Massnahme ausdrücklich nicht um eine Behandlung handelt, d. h. um die Anwendung von Pflanzenschutzmitteln auf Nutzpflanzen."@de,
        "Only use this class when the action is specifically not a treatment; i.e. the application of crop protection proucts on crops."@en .

srppp:ActionNotice a owl:Class ;
    rdfs:label "Hinweis bezüglich einer Schutzmassnahme"@de,
        "Notice regarding a protective action"@en ;
    rdfs:subClassOf srppp:GeneralNotice .

srppp:ActiveSubstance a owl:Class ;
    rdfs:label "Aktivsubstanz"@de,
        "Active substance"@en ;
    rdfs:comment "Aktivsubstanzen (auch Wirkstoffe genannt) sind biologisch aktive Verbindungen, die in geringen Konzentrationen biochemische und physiologische Prozesse in pflanzlichen, tierischen und menschlichen Organismen quantitativ und/oder qualitativ in Form von Hemmung oder Aktivierung beeinflussen."@de,
        "Active substances (also known as active ingredients) are biologically active compounds which, in low concentrations, influence biochemical and physiological processes in plant, animal and human organisms quantitatively and/or qualitatively in the form of inhibition or activation."@en ;
    rdfs:isDefinedBy <https://www.scienceindustries.ch/_file/27381/201903-begriffsklaerung-final-en.pdf> ;
    rdfs:subClassOf srppp:Substance ;
    skos:altLabel "Active ingredient",
        "Wirkstoff"@de .

srppp:AdditiveToDeclare a owl:Class ;
    rdfs:label "Zu deklarierende Additive"@de,
        "Additives to declare"@en ;
    rdfs:comment "Additive, die in einem Produkt enthalten sind und deklariert werden müssen, beispielsweise aufgrund ihrer chemischen Eigenschaften oder potenziellen Auswirkungen."@de,
        "Additives that are included in a product and must be declared, for example due to their chemical properties or potential effects."@en ;
    rdfs:subClassOf srppp:Substance .

srppp:AdhesionAgents a owl:Class ;
    rdfs:label "Netz- und Haftmittel"@de,
        "Wetting and adhesion agents"@en ;
    rdfs:comment "Netz- und Haftmittel sind chemische Produkte, die verwendet werden, um Pflanzenschutzmittel gleichmässig auf Pflanzenoberflächen zu verteilen und deren Anhaftung zu verbessern. Sie erhöhen die Effizienz und Wirksamkeit von Behandlungen."@de,
        "Wetting and adhesion agents are chemical products used to distribute crop protection products evenly on plant surfaces and enhance their adhesion. They improve the efficiency and effectiveness of treatments."@en ;
    rdfs:subClassOf srppp:ChemicalProduct .

srppp:BBCH a owl:Class ;
    rdfs:label "Anwendungszeitpunkt"@de,
        "Application time"@en ;
    rdfs:comment "Anwendungszeitpunkt für Behandlungen basierend auf der BBCH-Skala, die die phänologischen Entwicklungsstadien von Pflanzen standardisiert."@de,
        "Application time for treatments based on the BBCH scale, which standardizes the phenological development stages of plants."@en ;
    rdfs:seeAlso <https://w.wiki/CdkJ> ;
    rdfs:subClassOf srppp:TreatmentConstraint .

srppp:BCAAgainstFungi a owl:Class ;
    rdfs:label "Produkte gegen Pilze"@de,
        "Products against fungi"@en ;
    rdfs:comment "Produkte gegen Pilze sind biologische Schädlingsbekämpfungsmittel, die verwendet werden, um Pflanzen vor schädlichen Pilzen zu schützen. Sie basieren auf natürlichen Organismen oder deren Nebenprodukten und fördern nachhaltige landwirtschaftliche Praktiken."@de,
        "Products against fungi are biological control agents used to protect plants from harmful fungi. They are based on natural organisms or their byproducts and support sustainable agricultural practices."@en ;
    rdfs:subClassOf srppp:BiologicalControlAgent ;
    skos:altLabel "Antifungal biological agents"@en .

srppp:BCABacteria a owl:Class ;
    rdfs:label "Bakterienprodukte"@de,
        "Bacterial products"@en ;
    rdfs:comment "Bakterienprodukte sind biologische Schädlingsbekämpfungsmittel, die aus Bakterien bestehen oder von ihnen abgeleitet sind. Sie bekämpfen Schädlinge oder Krankheiten durch Mechanismen wie Konkurrenz, Produktion antimikrobieller Substanzen oder Förderung der Pflanzengesundheit."@de,
        "Bacterial products are biological control agents composed of or derived from bacteria. They combat pests or diseases through mechanisms such as competition, production of antimicrobial substances, or enhancement of plant health."@en ;
    rdfs:subClassOf srppp:BiologicalControlAgent .

srppp:BCAFungi a owl:Class ;
    rdfs:label "Pilzprodukte"@de,
        "Fungal products"@en ;
    rdfs:comment "Pilzprodukte sind biologische Schädlingsbekämpfungsmittel, die aus Pilzen bestehen oder von ihnen abgeleitet sind. Sie bekämpfen Schädlinge, Krankheiten oder Unkräuter durch Mechanismen wie Antagonismus, Parasitismus oder die Förderung der Pflanzengesundheit."@de,
        "Fungal products are biological control agents composed of or derived from fungi. They combat pests, diseases, or weeds through mechanisms such as antagonism, parasitism, or enhancement of plant health."@en ;
    rdfs:subClassOf srppp:BiologicalControlAgent .

srppp:BCAInsectViruses a owl:Class ;
    rdfs:label "Insektenvirenprodukte"@de,
        "Insect virus products"@en ;
    rdfs:comment "Insektenvirenprodukte sind biologische Schädlingsbekämpfungsmittel, die Viren enthalten, die gezielt Insekten schädigen. Sie bieten eine umweltfreundliche Alternative zu chemischen Insektiziden, da sie spezifisch auf Schadinsekten wirken und andere Organismen schonen."@de,
        "Insect virus products are biological control agents containing viruses that specifically harm insects. They provide an environmentally friendly alternative to chemical insecticides, as they target pests while being harmless to other organisms."@en ;
    rdfs:subClassOf srppp:BiologicalControlAgent .

srppp:BCAInsects a owl:Class ;
    rdfs:label "Insektenprodukte"@de,
        "Insect products"@en ;
    rdfs:subClassOf srppp:BiologicalControlAgent .

srppp:BCAMites a owl:Class ;
    rdfs:label "Milbenprodukte"@de,
        "Mite products"@en ;
    rdfs:subClassOf srppp:BiologicalControlAgent .

srppp:BCANematodes a owl:Class ;
    rdfs:label "Nematodenprodukte"@de,
        "Nematode products"@en ;
    rdfs:subClassOf srppp:BiologicalControlAgent .

srppp:Bactericide a owl:Class ;
    rdfs:label "Bakterizid"@de,
        "Bactericide"@en ;
    rdfs:comment "Ein Bakterizid ist ein chemisches Pflanzenschutzmittel, das verwendet wird, um bakterielle Krankheitserreger zu bekämpfen, die landwirtschaftliche Kulturen schädigen. Es wird häufig eingesetzt, um bakterielle Infektionen wie Feuerbrand zu verhindern oder zu kontrollieren."@de,
        "A bactericide is a chemical crop protection product used to combat bacterial pathogens that harm agricultural crops. It is often applied to prevent or control bacterial infections such as fire blight."@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:BestPracticeRecommendations a owl:Class ;
    rdfs:label "Schutzmassnahmenempfehlung"@de,
        "Protective Action Recommendation"@en ;
    rdfs:comment "Schutzmassnahmenempfehlungen geben Anleitung für die effektivsten und nachhaltigsten Massnahmen, um Schäden an spezifischen Nutzpflanzen zu verhindern oder zu minimieren, die durch bestimmte Stressfaktoren verursacht werden. Sie berücksichtigen die Art der Kultur, die Umweltbedingungen und die Besonderheiten des Stressfaktors."@de,
        "Protective action recommendations provide guidance on the most effective and sustainable measures to prevent or minimize damage to specific crops caused by particular stressors. They consider the type of crop, environmental conditions, and characteristics of the stressor."@en ;
    rdfs:subClassOf srppp:ActionNotice .

srppp:BiologicalControlAgent a owl:Class ;
    rdfs:label "Organismus für die biologische Schädlingsbekämpfung"@de,
        "Biological control agent"@en ;
    rdfs:subClassOf srppp:CropProtectionProduct ;
    owl:disjointWith srppp:ChemicalProduct .

srppp:BioticStressor a owl:Class ;
    rdfs:label "Biotischer Schadverursacher"@de,
        "Biotic crop stressor"@en ;
    rdfs:comment "Biologische Schädigung einer Kulturpflanze."@de,
        "Biological harm to a crop."@en ;
    rdfs:isDefinedBy wd:Q3500367 ;
    rdfs:seeAlso <https://w.wiki/Byym> ;
    rdfs:subClassOf srppp:CropStressor .

srppp:ChemicalCropProtectionProduct a owl:Class ;
    rdfs:label "Chemisches Pflanzenschutzmittel"@de,
        "Chemical crop protection product"@en ;
    rdfs:subClassOf srppp:ChemicalProduct,
        srppp:CropProtectionProduct ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( srppp:CropProtectionProduct srppp:ChemicalProduct ) ] .

srppp:ChemicalProduct a owl:Class ;
    rdfs:label "Chemisches Produkt"@de,
        "Chemical product"@en ;
    rdfs:comment "Jedes Produkt, das sich einigermassen gut durch seine (chemischen) Inhaltsstoffe beschreiben lässt."@de,
        "Any product that can be reasonably well described by its (chemical) ingredients."@en ;
    rdfs:subClassOf srppp:Product .

srppp:CodeR a owl:Class ;
    rdfs:label "R-Satz"@de,
        "R phrase"@en ;
    rdfs:comment "Ein R-Satz (= Risiko-Satz) ist eine standardisierte Risikobeschreibung für gefährliche chemische Stoffe, die spezifische Gefahren wie Toxizität oder Entflammbarkeit angibt. Diese wurden in der Europäischen Union verwendet, um Risiken auf Sicherheitsdatenblättern und Etiketten zu kennzeichnen."@de,
        "An R phrase (= risk phrase) is a standardized description of the risks associated with hazardous chemical substances, indicating specific dangers such as toxicity or flammability. These were used in the European Union to denote risks on safety data sheets and labels."@en ;
    rdfs:seeAlso <https://ec.europa.eu/taxation_customs/dds2/SAMANCTA/EN/Safety/RS_EN.htm>,
        <https://w.wiki/CjPj> ;
    rdfs:subClassOf srppp:HazardStatement .

srppp:CodeS a owl:Class ;
    rdfs:label "S-Satz"@de,
        "S phrase"@en ;
    rdfs:comment "Ein S-Satz (= Sicherheits-Satz) ist ein standardisierter Sicherheitshinweis für den Umgang mit gefährlichen chemischen Stoffen. Diese Sätze wurden in der Europäischen Union verwendet, um sichere Handhabung, Lagerung und Notfallmassnahmen auf Sicherheitsdatenblättern und Etiketten zu empfehlen."@de,
        "An S phrase (= safety phrase) is a standardized safety instruction for handling hazardous chemical substances. These phrases were used in the European Union to recommend safe handling, storage, and emergency measures on safety data sheets and labels."@en ;
    rdfs:seeAlso <https://ec.europa.eu/taxation_customs/dds2/SAMANCTA/EN/Safety/RS_EN.htm>,
        <https://w.wiki/CjPv> ;
    rdfs:subClassOf srppp:HazardStatement .

srppp:ComponentPortion a owl:Class ;
    rdfs:label "Komponentenportion"@de,
        "Component portion"@en ;
    rdfs:comment "Eine Komponentenportion stellt einen einzelnen Bestandteil eines Gemisches dar. Sie legt die Bestandssubstanz und deren Prozentsatz fest. Jede Komponentenportion muss genau eine Bestandssubstanz haben."@de,
        "A component portion represents a single constituent of a mixture, specifying the component substance and its percentage. Each component portion must have exactly one component substance."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty srppp:hasComponentSubstance ] .

srppp:ControlDamageThreshold a owl:Class ;
    rdfs:label "Bekämpfungsschadschwelle"@de,
        "Control Damage Threshold"@en ;
    rdfs:comment "Definiert die Schadensschwelle, ab der Massnahmen zur Bekämpfung eines Stressors ergriffen werden sollten, um weiteren Schaden zu verhindern."@de,
        "Defines the damage threshold at which control measures should be implemented to prevent further harm from a stressor."@en ;
    rdfs:subClassOf srppp:DamageThreshold .

srppp:CropGroup a owl:Class ;
    rdfs:label "Landwirtschaftliche Kultur"@de,
        "Crop"@en ;
    rdfs:comment "Repräsentiert eine Kategorie von landwirtschaftlichen Pflanzen, die innerhalb des Pflanzenschutzrahmens gemeinsame Merkmale, Anbaumethoden oder Verwendungszwecke teilen."@de,
        "Represents a category of agricultural plants that share common characteristics, cultivation methods, or purposes within the crop protection framework."@en ;
    rdfs:scopeNote "Entwickelt zur Modellierung von Kulturgruppen, die aufgrund von Ähnlichkeiten in ihren biologischen Eigenschaften, Wachstumsanforderungen oder den für ihren Schutz eingesetzten Strategien gemeinsam bewirtschaftet werden."@de,
        "Designed to model groups of crops that are managed collectively due to similarities in their biological attributes, growth requirements, or the strategies employed for their protection against stressors."@en .

srppp:CropProtectionProduct a owl:Class ;
    rdfs:label "Pflanzenschutzmittel"@de,
        "Crop protection product"@en ;
    rdfs:comment "Repräsentiert Produkte, die speziell entwickelt wurden, um Pflanzen vor Stressoren, einschliesslich biotischer und abiotischer Faktoren, im landwirtschaftlichen Rahmen zu schützen."@de,
        "Represents products specifically designed to protect crops from stressors, including both biotic and abiotic factors, within the agricultural framework."@en ;
    rdfs:scopeNote "Obwohl alle Pflanzenschutzmittel dieser Klasse zugeordnet werden, gehört nicht jedes Produkt zum Pflanzenschutz. Diese Klasse richtet sich speziell an Produkte, die entwickelt wurden, um Stressoren, die Pflanzen beeinträchtigen, zu mindern oder zu verwalten, wodurch sie sich von anderen Produkten wie Wachstumsregulatoren unterscheiden, die zwar Produkte sind, aber nicht zum Pflanzenschutz gehören."@de,
        "While all crop protection products are categorized under this class, not every product falls under plant protection. This class specifically targets products designed to mitigate or manage stressors affecting crops, distinguishing them from other products like growth regulators which, although they are products, do not fall under plant protection."@en ;
    rdfs:subClassOf srppp:Product .

srppp:CropStressor a owl:Class ;
    rdfs:label "Schadverursacher"@de,
        "Crop stressor"@en ;
    rdfs:comment "Repräsentiert jeden Faktor oder Agenten, der die Pflanzengesundheit, den Ertrag oder die Qualität im landwirtschaftlichen Rahmen negativ beeinflusst."@de,
        "Represents any factor or agent that negatively impacts crop health, yield, or quality within the agricultural framework."@en ;
    rdfs:scopeNote "Diese Klasse ist zur Modellierung verschiedener Stressoren vorgesehen, die Pflanzen betreffen, einschliesslich biotischer und abiotischer Elemente, um die Identifizierung und das Management von Faktoren zu erleichtern, die die landwirtschaftliche Produktivität beeinträchtigen."@de,
        "This class is intended to model various stressors affecting crops, including both biotic and abiotic elements, facilitating the identification and management of factors that compromise agricultural productivity."@en .

srppp:Damage a owl:Class ;
    rdfs:label "(Potenzieller) Schaden"@de,
        "(Potential) Damage"@en ;
    rdfs:comment "Repräsentiert den (potenziellen) Schaden, die einer spezifischen Kultur durch einen spezifischen Stressor zugefügt werden kann."@de,
        "Represents the adverse effects or harm (potentially) inflicted upon a specific crop due to a specific stressor."@en .

srppp:DamageThreshold a owl:Class ;
    rdfs:label "Schadschwelle"@de,
        "Damage threshold"@en ;
    rdfs:comment "Definiert die Schadensschwelle, ab der ein Stressor eine spezifische Kultur signifikant beeinträchtigt, und Massnahmen zur Reduzierung des Schadens erwogen werden sollten."@de,
        "Defines the damage threshold at which a stressor significantly impacts a specific crop, and measures to mitigate the damage should be considered."@en .

srppp:Desinfectant a owl:Class ;
    rdfs:label "Desinfektionsmittel"@de,
        "Desinfectant"@en ;
    rdfs:subClassOf srppp:ChemicalProduct .

srppp:EconomicDamageThreshold a owl:Class ;
    rdfs:label "Ökonomische Schadschwelle"@de,
        "Economic Damage Threshold"@en ;
    rdfs:comment "Definiert die Schadensschwelle, bei der der erwartete wirtschaftliche Schaden durch den Stressor die Kosten der Bekämpfungsmassnahmen übersteigt."@de,
        "Defines the damage threshold at which the expected economic harm caused by the stressor exceeds the cost of control measures."@en ;
    rdfs:subClassOf srppp:DamageThreshold .

srppp:EnvironmentalCondition a owl:Class ;
    rdfs:label "Umweltbedingung"@de,
        "Environmental condition"@en ;
    rdfs:comment "Repräsentiert jeden Umweltfaktor oder jede Umweltbedingung, die die Pflanzengesundheit, das Wachstum oder die Bewirtschaftung im landwirtschaftlichen Rahmen beeinflussen kann."@de,
        "Represents any environmental factor or situation that can influence crop health, growth, or management within agriculture."@en .

srppp:Fungicide a owl:Class ;
    rdfs:label "Fungizid"@de,
        "Fungicide"@en ;
    rdfs:comment "Ein Fungizid ist ein chemisches Pflanzenschutzmittel, das zur Bekämpfung von Pilzkrankheiten in der Landwirtschaft eingesetzt wird. Es tötet Pilze oder hemmt ihr Wachstum, um Ernteerträge und -qualität zu schützen."@de,
        "A fungicide is a chemical crop protection product used to combat fungal diseases in agriculture. It kills fungi or inhibits their growth to protect crop yields and quality."@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:GeneralNotice a owl:Class ;
    rdfs:label "Allgemeiner Hinweis"@de,
        "General Notice"@en .

srppp:HazardStatement a owl:Class ;
    rdfs:label "Sicherheitshinweise für Gefahrstoffe"@de,
        "Hazard statements"@en ;
    rdfs:comment "H-Sätze (Gefahrenhinweise) und die ergänzenden EUH-Sätze sind standardisierte Beschreibungen der spezifischen Gefahren, die von chemischen Stoffen oder Gemischen ausgehen, wie z. B. Toxizität, Entflammbarkeit oder Umweltschädlichkeit. Sie sind Teil des Global Harmonisierten Systems (GHS) und gewährleisten eine klare, einheitliche Kommunikation chemischer Gefahren weltweit."@de,
        "H-statements (Hazard Statements) and the complementary EUH-statements are standardized descriptions of the specific dangers posed by chemical substances or mixtures, such as toxicity, flammability, or environmental harm. They are part of the Globally Harmonized System (GHS) and ensure clear, consistent communication of chemical hazards worldwide."@en ;
    rdfs:seeAlso <https://w.wiki/CVwr> ;
    rdfs:subClassOf srppp:GeneralNotice .

srppp:Herbicide a owl:Class ;
    rdfs:label "Herbizid"@de,
        "Herbicide"@en ;
    rdfs:comment "Ein Herbizid ist ein chemisches Pflanzenschutzmittel, das zur Bekämpfung unerwünschter Pflanzen (Unkräuter) eingesetzt wird. Es hilft, das Wachstum von Unkräutern zu hemmen oder sie abzutöten, um die Erträge und Qualität der Kulturpflanzen zu schützen."@de,
        "A herbicide is a chemical crop protection product used to control unwanted plants (weeds). It helps inhibit the growth of weeds or eliminate them to protect the yields and quality of cultivated crops."@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:Insecticide a owl:Class ;
    rdfs:label "Insektizid"@de,
        "Insecticide"@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:Mixture a owl:Class ;
    rdfs:label "Chemisches Gemisch"@de,
        "Chemical mixture"@en ;
    rdfs:comment "Ein chemisches Gemisch ist eine Art von Substanz, die aus mindestens zwei Komponentenportionen besteht, von denen jede eine Bestandssubstanz mit einem bestimmten Prozentsatz repräsentiert."@de,
        "A chemical mixture is a type of substance that consists of at least two component portions, each representing a constituent substance with a specified percentage."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onProperty srppp:hasComponentPortion ],
        srppp:Substance .

srppp:Molluscicide a owl:Class ;
    rdfs:label "Molluskizid"@de,
        "Molluscicide"@en ;
    rdfs:comment "Ein Molluskizid ist ein chemisches Pflanzenschutzmittel, das zur Bekämpfung von Weichtieren, insbesondere Schnecken und Nacktschnecken, eingesetzt wird. Es schützt landwirtschaftliche Kulturen vor Schäden, die durch diese Schädlinge verursacht werden."@de,
        "A molluscicide is a chemical crop protection product used to control mollusks, particularly slugs and snails. It protects agricultural crops from damage caused by these pests."@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:NaturalDefenseStimulator a owl:Class ;
    rdfs:label "Stimulator der natürlichen Abwehrkräfte"@de,
        "Stimulator of natural defenses"@en ;
    rdfs:comment "Ein Stimulator der natürlichen Abwehrkräfte ist ein chemisches Produkt, das die natürlichen Abwehrmechanismen von Pflanzen aktiviert. Durch die Stimulierung des pflanzeneigenen Immunsystems erhöhen diese Produkte die Resistenz der Pflanzen gegen Krankheiten und Schädlinge, ohne direkt auf die Pathogene einzuwirken."@de,
        "A stimulator of natural defenses is a chemical product that activates the natural defense mechanisms of plants. By stimulating the plant's own immune system, these products enhance the plant's resistance to diseases and pests without directly targeting the pathogens."@en ;
    rdfs:subClassOf srppp:ChemicalProduct .

srppp:Nematicide a owl:Class ;
    rdfs:label "Nematizid"@de,
        "Nematicide"@en ;
    rdfs:comment "Ein Nematizid ist ein chemisches Pflanzenschutzmittel, das zur Bekämpfung von Nematoden (Fadenwürmern) eingesetzt wird. Diese Mittel sind entscheidend für den Schutz von Kulturpflanzen, da Nematoden erhebliche Schäden an Wurzelsystemen verursachen und die Ernteerträge mindern können."@de,
        "A nematicide is a chemical crop protection product used to control nematodes (roundworms). These agents are crucial for protecting cultivated plants, as nematodes can cause significant damage to root systems and reduce crop yields."@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:Obligation a owl:Class ;
    rdfs:label "Auflage zu Behandlungen"@de,
        "Application requirement"@en ;
    rdfs:comment "Eine Auflage ist eine spezifische Bedingung oder Anforderung, die für die sachgemässe Anwendung eines Pflanzenschutzmittels gemäss dem Schweizer Pflanzenschutzmittelverzeichnis erfüllt werden muss. Diese Auflagen sollen die sichere und effektive Verwendung des Produkts gewährleisten und Risiken für Mensch, Tier und Umwelt minimieren."@de,
        "An application requirement is a specific condition or obligation that must be met for the proper use of a plant protection product according to the Swiss Plant Protection Product Register. These requirements are intended to ensure the safe and effective use of the product, minimizing risks to human health, animals, and the environment."@en ;
    rdfs:subClassOf srppp:ActionNotice .

srppp:ParallelImport a owl:Class ;
    rdfs:label "Parallelimport"@de,
        "Parallel import"@en ;
    rdfs:comment "Ein Parallelimport bezeichnet den Import von Pflanzenschutzmitteln, die in einem Land rechtmässig zugelassen sind und ohne ausdrückliche Zustimmung des Rechteinhabers in ein anderes Land eingeführt werden, sofern sie in Zusammensetzung und Zulassungsanforderungen einem bereits im Einfuhrland zugelassenen Produkt entsprechen. In der Schweiz erfordert ein solcher Import eine Generaleinfuhrbewilligung sowie die Einhaltung spezifischer Vorschriften zu Kennzeichnung, Verpackung und Verwendung gemäss dem schweizerischen Referenzprodukt. Dieses System gewährleistet die Verfügbarkeit von Produkten und berücksichtigt gleichzeitig Sicherheits-, Zulassungs- und Schutzrechte des geistigen Eigentums."@de,
        "A parallel import refers to the importation of pesticide products that are legally authorized in one country and are brought into another without the direct consent of the intellectual property holder, provided they meet the same composition and regulatory standards as a product already authorized in the importing country. In Switzerland, such imports require a general import permit and compliance with specific labeling, packaging, and usage regulations aligned with the Swiss-approved reference product. This system ensures product availability while maintaining safety, regulatory adherence, and respect for intellectual property rights."@en ;
    rdfs:seeAlso <https://www.fedlex.admin.ch/eli/cc/2010/340/de#art_36> ;
    rdfs:subClassOf srppp:Product .

srppp:Pheromone a owl:Class ;
    rdfs:label "Pheromon"@de,
        "Pheromone"@en ;
    rdfs:comment "Pheromone sind chemische Substanzen, die von Insekten abgesondert werden, um mit Artgenossen zu kommunizieren. Sie beeinflussen Verhaltensweisen wie Paarung, Aggregation oder Spurverfolgung. In der Schädlingsbekämpfung werden synthetische Pheromone als Lockstoffe eingesetzt, um Schädlingspopulationen zu überwachen oder zu kontrollieren."@de,
        "Pheromones are chemical substances secreted by insects to communicate with conspecifics. They influence behaviors such as mating, aggregation, or trail following. In pest management, synthetic pheromones are used as attractants to monitor or control pest populations."@en ;
    rdfs:subClassOf srppp:Insecticide ;
    skos:altLabel "Insektenlockstoffe"@de,
        "Insect Attractant"@en .

srppp:Phytoregulator a owl:Class ;
    rdfs:label "Phytoregulator"@de,
        "Phytoregulator"@en ;
    rdfs:subClassOf srppp:ChemicalProduct .

srppp:Product a owl:Class ;
    rdfs:label "Produkt"@de,
        "Product"@en ;
    rdfs:comment "Repräsentiert jedes Element im Rahmen des Pflanzenschutzes, das mit einer eidgenössischen Zulassungsnummer registriert ist und spezifische Berechtigungen hinsichtlich der nicht-beruflichen Verwendung aufweist."@de,
        "Represents any item within the crop protection framework that is registered with a federal admission number and has specified permissions regarding non-professional use."@en ;
    rdfs:scopeNote "Diese Klasse ist für die Modellierung aller Pflanzenschutzmittel vorgesehen, die eine eidgenössische Registrierung erfordern und Einschränkungen oder Berechtigungen für die nicht-berufliche Nutzung gemäss den Schweizer Vorschriften aufweisen können."@de,
        "This class is intended for modeling all crop protection products requiring federal registration and that may have restrictions or allowances for non-professional usage as defined by Swiss regulations."@en ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty srppp:isNonProfessionallyAllowed ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty srppp:hasFederalAdmissionNumber ] .

srppp:PureSubstance a owl:Class ;
    rdfs:label "Reine Substanz"@de,
        "Pure substance"@en ;
    rdfs:comment "Eine reine chemische Substanz ist mit genau einer CheBI-Chemikalie verknüpft und kann kein Gemisch sein. Reine Substanzen und chemische Gemische sind disjunkte Klassen."@de,
        "A pure chemical substance is associated with exactly one CheBI chemical entity and cannot be a mixture. Pure substances and chemical mixtures are disjoint classes."@en ;
    rdfs:subClassOf srppp:Substance ;
    owl:disjointWith srppp:Mixture .

srppp:Rodenticide a owl:Class ;
    rdfs:label "Rodentizid"@de,
        "Rodenticide"@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:SeedDressing a owl:Class ;
    rdfs:label "Saatbeizmittel"@de,
        "Seed dressing"@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:SoapProduct a owl:Class ;
    rdfs:label "Seifenpräparat-Insektizide"@de,
        "Soap product insecticides"@en ;
    rdfs:subClassOf srppp:Insecticide .

srppp:StorageProtectors a owl:Class ;
    rdfs:label "Vorratsschutzmittel"@de,
        "Storage protectors"@en ;
    rdfs:subClassOf srppp:ChemicalProduct .

srppp:Substance a owl:Class ;
    rdfs:label "Wirkstoff"@de,
        "Substance"@en ;
    rdfs:comment "Eine chemische Substanz, die entweder eine reine Substanz oder ein Gemisch aus anderen Substanzen sein kann. Jede Substanz kann mit höchstens einer CheBI-Chemikalie verknüpft sein."@de,
        "A chemical substance, which may be either a pure substance or a mixture composed of other substances. Each substance can be associated with at most one CheBI chemical entity."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty srppp:hasChebiIdentity ] .

srppp:Synergist a owl:Class ;
    rdfs:label "Synergist"@de,
        "Synergist"@en ;
    rdfs:comment "Ein Synergist ist eine Substanz, die selbst keine oder nur geringe insektizide Wirkung besitzt, aber die Wirksamkeit von Insektiziden erhöht. Synergisten wirken, indem sie die Entgiftungsmechanismen von Schädlingen hemmen, wodurch die Toxizität der Insektizide verstärkt wird."@de,
        "A synergist is a substance that has little or no insecticidal effect on its own but enhances the efficacy of insecticides. Synergists function by inhibiting the detoxification mechanisms of pests, thereby increasing the toxicity of the insecticides."@en ;
    rdfs:subClassOf srppp:Substance .

srppp:Treatment a owl:Class ;
    rdfs:label "Behandlung"@de,
        "Treatment"@en ;
    rdfs:subClassOf srppp:Action .

srppp:TreatmentConstraint a owl:Class ;
    rdfs:label "Anwendungseinschränkung"@de,
        "Treatment constraint"@en ;
    rdfs:subClassOf srppp:ActionNotice .

srppp:TreatmentDosage a owl:Class ;
    rdfs:label "Dosierung"@de,
        "Dosage"@en ;
    rdfs:subClassOf srppp:TreatmentConstraint .

srppp:TreatmentEfficacy a owl:Class ;
    rdfs:label "Behandlungswirkung"@de,
        "Treatment efficacy"@en .

srppp:TreatmentRate a owl:Class ;
    rdfs:label "Anwendungsrate"@de,
        "Application rate"@en ;
    rdfs:comment "Anwendungsrate für Behandlungen."@de,
        "Application rate for treatments."@en ;
    rdfs:subClassOf srppp:TreatmentConstraint .

srppp:Viricide a owl:Class ;
    rdfs:label "Virizid"@de,
        "Viricide"@en ;
    rdfs:subClassOf srppp:ChemicalCropProtectionProduct .

srppp:WaitingPeriod a owl:Class ;
    rdfs:label "Wartefrist"@de,
        "Waiting period"@en ;
    rdfs:comment "Repräsentiert das erforderliche Zeitintervall nach einer Behandlung, in dem dasselbe Produkt nicht erneut auf dieselbe Kulturpflanze angewendet werden sollte."@de,
        "Represents the required time interval after a treatment during which the same product should not be applied again to the same crop."@en ;
    rdfs:subClassOf srppp:TreatmentConstraint .

srppp:nonTargetOrganism a owl:Class ;
    rdfs:label "Nichtzielorganismus"@de,
        "Non target organism"@en ;
    rdfs:comment "Jeder Organismus ausser dem, gegen den das Pestizid angewendet wird."@de,
        "Any organism other than the one against which the pesticide is applied."@en .

<https://schema.org/Country> a owl:Class ;
    rdfs:label "Land"@de,
        "Country"@en .

<https://schema.org/Organization> a owl:Class ;
    rdfs:label "Organisation"@de,
        "Organization"@en ;
    rdfs:comment "Repräsentiert jede formelle Einheit, wie Unternehmen, Institutionen oder Vereinigungen, die im landwirtschaftlichen Bereich des Pflanzenschutzrahmens tätig sind."@de,
        "Represents any formal entity, such as companies, institutions, or associations, involved in the agricultural sector within the crop protection framework."@en ;
    rdfs:scopeNote "Diese Klasse ist zur Modellierung von Organisationen im Pflanzenschutz vorgesehen. Sofern möglich, sollten Instanzen aus dem Schweizer zentralen Firmenindex (Zefix) wiederverwendet werden, um Konsistenz und Integration mit bestehenden Schweizer Organisationsdaten zu gewährleisten."@de,
        "This class is intended to model organizations involved in crop protection. Whenever possible, instances from the Swiss central company index (Zefix) should be reused to ensure consistency and integration with existing Swiss organizational data."@en .

<https://schema.org/PostalAddress> a owl:Class ;
    rdfs:label "Postadresse"@de,
        "Postal address"@en .

srppp:componentPercentage a owl:DatatypeProperty ;
    rdfs:label "Komponentenprozentsatz"@de,
        "Component percentage"@en ;
    rdfs:comment "Gibt den Prozentsatz (0% - 100%) der Bestandssubstanz innerhalb eines Gemisches an."@de,
        "Specifies the percentage (0% - 100%) of the component substance within a mixture."@en ;
    rdfs:domain srppp:ComponentPortion ;
    rdfs:range xsd:float .

srppp:earliesBBCH a owl:DatatypeProperty ;
    rdfs:label "Frühestes BBCH-Stadium der Kulturpflanze"@de,
        "Earlies BBCH stage of the crop"@en ;
    rdfs:comment "Das früheste BBCH-Stadium der Kulturpflanze, bei welcher der Schaden schon auftreten kann."@de,
        "The earliest BBCH stage of the crop at which the damage can already occur."@en ;
    rdfs:domain srppp:Damage ;
    rdfs:range xsd:float .

srppp:emoji a owl:DatatypeProperty ;
    rdfs:label "Emoji"@de,
        "Emoji"@en ;
    rdfs:comment "Definiert ein Emoji, das als symbolische Darstellung eines Dings dienen kann."@de,
        "Defines an emoji that can serve as a symbolic representation of a thing."@en ;
    rdfs:domain rdfs:Resource ;
    rdfs:range xsd:string ;
    rdfs:scopeNote "Gedacht für die Verwendung bei Instanzen von Klassen, die sich wie Codelisten verhalten."@de,
        "Intended for use with instances of classes that function similarly to enumerated types."@en .

srppp:hasDamageThreshold a owl:DatatypeProperty ;
    rdfs:label "hat Schadschwelle"@de,
        "has damage threshold"@en ;
    rdfs:domain srppp:Damage ;
    rdfs:range srppp:DamageThreshold .

srppp:hasExhaustionDeadline a owl:DatatypeProperty ;
    rdfs:label "Ablaufdatum der Nutzung"@de,
        "Exhaustion deadline"@en ;
    rdfs:comment "Das Datum, an dem das Produkt nicht mehr genutzt werden darf, in der Regel nach der Widerrufung der Produktregistrierung."@de,
        "The date by which the product must no longer be used, typically after the product registration has been revoked."@en ;
    rdfs:domain srppp:Product ;
    rdfs:range xsd:date .

srppp:hasFederalAdmissionNumber a owl:DatatypeProperty ;
    rdfs:label "Eidgenössische Zulassungsnummer"@de,
        "Federal admission number"@en ;
    rdfs:comment "Die eidgenössische Zulassungsnummer identifiziert eine Produktregistrierung eindeutig. Sie ist entweder als W-Nummer im Fall von Bundeszulassungen (z. B. W-18) oder als Parallelimportnummer mit dem Anfangsbuchstaben des Ursprungslandes im Code im Fall von Parallelimporten (z. B. I-2789 für einen Parallelimport aus Italien) aufgebaut. Nummern mit einem Bindestrich (z. B. W-18-1, W-18-2) stehen für gleichwertige Produkte, die sich im Besitz verschiedener juristischer Personen befinden, während Nummern ohne Bindestrich (z. B. W-18) für die ursprüngliche Registrierung stehen."@de,
        "The federal admission number uniquely identifies a product registration. It is either constructed as a W-Number in the case of federal admissions (e.g. W-18) or as a parallel import number with the country of origin starting letter in the code in the case of parallel imports (e.g. I-2789 for a parallel import from Italy). Numbers with a dash (e.g., W-18-1, W-18-2) represent equivalent products held by different legal entities, while numbers without a dash (e.g., W-18) represent the original registration."@en ;
    rdfs:domain srppp:Product ;
    rdfs:range xsd:NCName .

srppp:hasForeignAdmissionNumber a owl:DatatypeProperty ;
    rdfs:label "Ausländische Zulassungsnummer"@de,
        "Foreign admission number"@en ;
    rdfs:comment "Diese Eigenschaft gibt die Zulassungs- oder Registrierungsnummer an, die dem Produkt in seinem Herkunftsland zugewiesen wurde."@de,
        "This property specifies the admission or registration number assigned to the product in its country of origin."@en ;
    rdfs:domain srppp:ParallelImport ;
    rdfs:range xsd:string .

srppp:hasHazardStatementCode a owl:DatatypeProperty ;
    rdfs:label "Code bezüglich Sicherheitshinweisen für Gefahrstoffe"@de,
        "Hazard statement code"@en ;
    rdfs:domain srppp:HazardStatement ;
    rdfs:range xsd:string .

srppp:hasSoldoutDeadline a owl:DatatypeProperty ;
    rdfs:label "Ausverkauft-Frist"@de,
        "Sold out deadline"@en ;
    rdfs:comment "Das Datum, an dem das Produkt nicht mehr verkauft werden darf, in der Regel nach der Widerrufung der Produktregistrierung."@de,
        "The date by which the product can no longer be sold, typically after the product registration has been revoked."@en ;
    rdfs:domain srppp:Product ;
    rdfs:range xsd:date .

srppp:hasTreatmentEfficacy a owl:DatatypeProperty ;
    rdfs:label "hat Behandlungswirkung"@de,
        "has treatment efficacy"@en ;
    rdfs:domain srppp:Treatment ;
    rdfs:range srppp:TreatmentEfficacy .

srppp:hasUpperDamageLimit a owl:DatatypeProperty ;
    rdfs:label "Obere Schadenslimite"@de,
        "Upper damage limit"@en ;
    rdfs:comment "Gibt die Schadensgrenze in Prozent (0% -100%) an, die ein Stressfaktor für Nutzpflanzen vernünftigerweise verursachen kann."@de,
        "Indicates the damage limit in percentage (0% -100%) that a crop stressor can reasonably cause."@en ;
    rdfs:domain srppp:Damage ;
    rdfs:range xsd:float .

srppp:isNonProfessionallyAllowed a owl:DatatypeProperty ;
    rdfs:label "Erlaubnis für die nicht-berufliche Verwendung"@de,
        "Allowance for non-professional use"@en ;
    rdfs:comment "Diese Eigenschaft gibt an, ob ein Produkt für die nicht-berufliche Verwendung zugelassen ist, wie in Artikel 64 der Schweizer Pflanzenschutzmittelverordnung (PSMV) definiert."@de,
        "This property indicates whether a product is permitted for non-professional use, as defined by Article 64 of the Swiss Plant Protection Products Ordinance (PSMV)."@en ;
    rdfs:domain srppp:Product ;
    rdfs:range xsd:boolean ;
    rdfs:seeAlso <https://www.fedlex.admin.ch/eli/cc/2010/340/de#art_64> .

srppp:iupac a owl:DatatypeProperty ;
    rdfs:label "IUPAC-Name"@de,
        "IUPAC name"@en ;
    rdfs:comment "Systematische Bezeichnung der chemischen Verbindung gemäss den Regeln der IUPAC‑Nomenklatur."@de,
        "The systematic IUPAC name of the chemical substance, following IUPAC nomenclature rules."@en ;
    rdfs:domain srppp:Substance ;
    rdfs:range xsd:string .

srppp:latestBBCH a owl:DatatypeProperty ;
    rdfs:label "Spätestes BBCH-Stadium der Kulturpflanze"@de,
        "Latest BBCH stage of the crop"@en ;
    rdfs:comment "Das späteste BBCH-Stadium der Kulturpflanze, bei welcher der Schaden noch auftreten kann."@de,
        "The latest BBCH stage of the crop at which the damage can still occur."@en ;
    rdfs:domain srppp:Damage ;
    rdfs:range xsd:float .

srppp:maximumBBCHcrop a owl:DatatypeProperty ;
    rdfs:label "Maximales BBCH-Stadium der Kulturpflanze"@de,
        "Maximal BBCH stadium of the crop"@en ;
    rdfs:domain srppp:BBCH ;
    rdfs:range xsd:float .

srppp:maximumBBCHweed a owl:DatatypeProperty ;
    rdfs:label "Maximales BBCH-Stadium des Unkrautes"@de,
        "Maximal BBCH stadium of the weed"@en ;
    rdfs:domain srppp:BBCH ;
    rdfs:range xsd:float .

srppp:maximumRate a owl:DatatypeProperty ;
    rdfs:label "Maximale Anwendungsrate"@de,
        "Maximal application rate"@en ;
    rdfs:comment "Gibt an, wie viel von diesem Produkt in einer Saison maximal verwendet werden darf."@de,
        "Indicates how much of a given product may be maximally applied per season."@en ;
    rdfs:domain srppp:TreatmentRate ;
    rdfs:range xsd:float .

srppp:maximumTreatmentDosage a owl:DatatypeProperty ;
    rdfs:label "Maximale Dosierung"@de,
        "Maximal dosage"@en ;
    rdfs:domain srppp:TreatmentDosage ;
    rdfs:range xsd:float .

srppp:minimumBBCHcrop a owl:DatatypeProperty ;
    rdfs:label "Minimales BBCH-Stadium der Kulturpflanze"@de,
        "Minimal BBCH stage of the crop"@en ;
    rdfs:comment "Das minimale BBCH-Stadium der Kulturpflanze bei der Anwendung des Produktes."@de,
        "The minimal BBCH stage of the crop when applying the product."@en ;
    rdfs:domain srppp:BBCH ;
    rdfs:range xsd:float .

srppp:minimumBBCHweed a owl:DatatypeProperty ;
    rdfs:label "Minimales BBCH-Stadium der Kulturpflanze"@de,
        "Minimal BBCH stage of the crop"@en ;
    rdfs:comment "Das minimale BBCH-Stadium des Zielorganismus (Unkraut) bei der Anwendung des Produktes."@de,
        "The minimal BBCH stage of the target organism (weed) when applying the product."@en ;
    rdfs:domain srppp:BBCH ;
    rdfs:range xsd:float .

srppp:minimumRate a owl:DatatypeProperty ;
    rdfs:label "Minimale Anwendungsrate"@de,
        "Minimal application rate"@en ;
    rdfs:domain srppp:TreatmentRate ;
    rdfs:range xsd:float .

srppp:minimumTreatmentDosage a owl:DatatypeProperty ;
    rdfs:label "Minimale Dosierung"@de,
        "Minimal dosage"@en ;
    rdfs:domain srppp:TreatmentDosage ;
    rdfs:range xsd:float .

srppp:minimumWaitingPeriod a owl:DatatypeProperty ;
    rdfs:label "Minimale Wartezeit"@de,
        "Minimal waiting period"@en ;
    rdfs:domain srppp:WaitingPeriod ;
    rdfs:range xsd:float .

<https://schema.org/addressLocality> a owl:DatatypeProperty ;
