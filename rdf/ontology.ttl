    rdfs:domain <https://schema.org/PostalAddress> ;
    rdfs:range xsd:string .

<https://schema.org/telephone> a owl:DatatypeProperty ;
    rdfs:label "Telefonnummer"@de,
        "Phone number"@en ;
    rdfs:comment "Die Telephonnummer des Unternehmens, formatiert gemäss RFC 3966."@de,
        "The telephone number of the company, formatted as per RFC 3966."@en ;
    rdfs:domain <https://schema.org/Organization> ;
    rdfs:isDefinedBy <https://www.rfc-editor.org/rfc/rfc3966> ;
    rdfs:range xsd:string .

wdt:P171 a owl:ObjectProperty ;
    rdfs:label "hat übergeordnetes Taxon"@de,
        "has parent taxon"@en ;
    rdfs:comment "Nächsthöheres Taxon nach einem bestimmten taxonomischen Gesichtspunkt. Da es mehrere taxonomische Gesichtspunkte gibt, ist mehr als ein übergeordnetes Taxon möglich."@de,
        "Next higher taxon according to a certain taxonomic viewpoint. As multiple taxonomic viewpoints exist, more than one parent taxon is possible."@en ;
    rdfs:domain wd:Q16521 ;
    rdfs:range wd:Q16521 .

srppp:BCAisDefinedByBiologicalTaxon a owl:ObjectProperty ;
    rdfs:label "ist definiert durch das biologische Taxon"@de,
        "is defined by biological taxon"@en ;
    rdfs:comment "Ein Organismus für die biologische Schädlingsbekämpfung gehört zu einem bestimmten biologischen Taxon."@de,
        "A biological control agent belongs to a certain biological taxon."@en ;
    rdfs:domain srppp:BiologicalControlAgent ;
    rdfs:range wd:Q16521 ;
    rdfs:subPropertyOf srppp:isDefinedByBiologicalTaxon .

srppp:NonTargetOrganismIsDefinedByBiologicalTaxon a owl:ObjectProperty ;
    rdfs:label "ist definiert durch das biologische Taxon"@de,
        "is defined by biological taxon"@en ;
    rdfs:domain srppp:nonTargetOrganism ;
    rdfs:range wd:Q16521 ;
    rdfs:subPropertyOf srppp:isDefinedByBiologicalTaxon .

srppp:affects a owl:ObjectProperty ;
    rdfs:label "betrifft"@de,
        "affects"@en ;
    rdfs:domain srppp:Damage ;
    rdfs:range srppp:CropGroup .

srppp:appliesToProduct a owl:ObjectProperty ;
    rdfs:label "bezieht sich auf das Produkt"@de,
        "applies to product"@en ;
    rdfs:comment "Diese Eigenschaft verknüpft eine Gefahrenkennzeichnung mit dem Produkt, auf das sie sich bezieht."@de,
        "This property associates a hazard statement with the product it pertains to."@en ;
    rdfs:domain srppp:HazardStatement ;
    rdfs:range srppp:Product ;
    owl:inverseOf srppp:hasHazardStatement .

srppp:bioticStressorIsDefinedByBiologicalTaxon a owl:ObjectProperty ;
    rdfs:label "ist definiert durch das biologische Taxon"@de,
        "is defined by biological taxon"@en ;
    rdfs:comment "Diese Eigenschaft verknüpft einen biotischen Stressor mit dem biologischen Taxon, das ihn definiert."@de,
        "This property links a biotic stressor to the biological taxon that defines it."@en ;
    rdfs:domain srppp:BioticStressor ;
    rdfs:range wd:Q16521 ;
    rdfs:subPropertyOf srppp:isDefinedByBiologicalTaxon .

srppp:causes a owl:ObjectProperty ;
    rdfs:label "verursacht"@de,
        "causes"@en ;
    rdfs:domain srppp:CropStressor ;
    rdfs:range srppp:Damage .

srppp:causesCollateralDamageTo a owl:ObjectProperty ;
    rdfs:label "führt zu einem Kollateralschaden bei"@de,
        "causes collateral damage to"@en ;
    rdfs:comment "Diese Eigenschaft zeigt an, dass die Anwendung eines Produkts unbeabsichtigte Schäden bei Nicht-Zielorganismen verursachen kann."@de,
        "This property signifies that the application of a product may lead to unintended harm to non-target organisms."@en ;
    rdfs:domain srppp:Product ;
    rdfs:range srppp:nonTargetOrganism .

srppp:chemicallyDefines a owl:ObjectProperty ;
    rdfs:label "definiert chemisch"@de,
        "chemically defines"@en ;
    rdfs:domain srppp:Substance ;
    rdfs:range srppp:ChemicalProduct .

srppp:concerns a owl:ObjectProperty ;
    rdfs:label "betrifft"@de,
        "concerns"@en ;
    rdfs:domain srppp:ActionNotice ;
    rdfs:range srppp:Action .

srppp:contains a owl:ObjectProperty ;
    rdfs:label "beinhaltet"@de,
        "contains"@en ;
    rdfs:comment "Diese Eigenschaft verknüpft ein chemisches Produkt mit reinen chemischen Substanzen. Sie dient der einfachen Überprüfung, ob ein chemisches Produkt eine bestimmte reine chemische Substanz enthält."@de,
        "This property links a chemical product to pure chemical substances. It exists for simple checks wether a chemical product contains a certain pure chemical substance."@en ;
    rdfs:domain srppp:ChemicalProduct ;
    rdfs:range srppp:PureSubstance .

srppp:cropIsRelatedToBiologicalTaxon a owl:ObjectProperty ;
    rdfs:label "hängt zusammen mit dem biologischen Taxon"@de,
        "is related to biological taxon"@en ;
    rdfs:comment "Diese Eigenschaft verknüpft eine landwirtschaftliche Kultur mit dem biologischen Taxon, mit dem sie verbunden ist."@de,
        "This property links a crop to the biological taxon it is associated with."@en ;
    rdfs:domain srppp:CropGroup ;
    rdfs:range wd:Q16521 ;
    rdfs:subPropertyOf srppp:isDefinedByBiologicalTaxon .

srppp:damages a owl:ObjectProperty ;
    rdfs:label "beschädigt"@de,
        "damages"@en ;
    rdfs:domain srppp:CropStressor ;
    rdfs:range srppp:CropGroup ;
    rdfs:subPropertyOf srppp:interactsWith .

srppp:enables a owl:ObjectProperty ;
    rdfs:label "ermöglicht"@de,
        "enables"@en ;
    rdfs:comment "Diese Eigenschaft zeigt an, dass eine bestimmte Umweltbedingung eine bestimmte Art von Schaden ermöglicht oder verursacht."@de,
        "This property signifies that a specific environmental condition facilitates or leads to a particular type of damage."@en ;
    rdfs:domain srppp:EnvironmentalCondition ;
    rdfs:range srppp:Damage .

srppp:hasChebiIdentity a owl:ObjectProperty ;
    rdfs:label "hat CheBI-Identität"@de,
        "has CheBI identity"@en ;
    rdfs:comment "Verknüpft eine Substanz mit der entsprechenden chemischen Entität in der CheBI-Ontologie und erleichtert so eine standardisierte chemische Identifizierung."@de,
        "Associates a substance with its corresponding chemical entity in the CheBI ontology, facilitating standardized chemical identification."@en ;
    rdfs:domain srppp:Substance ;
    rdfs:range chebi:24431 .

srppp:hasChildCropGroup a owl:ObjectProperty ;
    rdfs:label "hat Untergruppe"@de,
        "has child crop group"@en ;
    rdfs:comment "Diese Eigenschaft zeigt an, dass eine Kulturgruppe eine andere Kulturgruppe als Untergruppe enthält."@de,
        "This property indicates that a crop group includes another crop group as its subgroup."@en ;
    rdfs:domain srppp:CropGroup ;
    rdfs:range srppp:CropGroup ;
    owl:inverseOf srppp:hasParentCropGroup .

srppp:hasComponentPortion a owl:ObjectProperty ;
    rdfs:label "hat Komponentenportion"@de,
        "has component portion"@en ;
    rdfs:comment "Verknüpft ein Gemisch mit einer seiner Komponentenportionen, wobei jede Portion eine Bestandssubstanz und deren Prozentsatz angibt."@de,
        "Links a mixture to one of its component portions, each detailing a component substance and its percentage."@en ;
    rdfs:domain srppp:Mixture ;
    rdfs:range srppp:ComponentPortion .

srppp:hasComponentSubstance a owl:ObjectProperty ;
    rdfs:label "hat Bestandssubstanz"@de,
        "has component substance"@en ;
    rdfs:comment "Verknüpft eine Komponentenportion mit der spezifischen Substanz, die als Bestandteil in einem Gemisch dient."@de,
        "Links a component portion to the specific substance that serves as a component within a mixture."@en ;
    rdfs:domain srppp:ComponentPortion ;
    rdfs:range srppp:Substance .

srppp:hasCountryOfOrigin a owl:ObjectProperty ;
    rdfs:label "hat Herkunftsland"@de,
        "has country of origin"@en ;
    rdfs:comment "Diese Eigenschaft gibt das Land an, aus dem das Produkt stammt."@de,
        "This property indicates the country where the product originates."@en ;
    rdfs:domain srppp:Product ;
    rdfs:range <https://schema.org/Country> .

srppp:hasHazardStatement a owl:ObjectProperty ;
    rdfs:label "hat Sicherheitshinweis für Gefahrenstoff"@de,
        "has hazard statements"@en ;
    rdfs:domain srppp:Product ;
    rdfs:range srppp:HazardStatement .

srppp:hasParentCropGroup a owl:ObjectProperty ;
    rdfs:label "hat Übergruppe"@de,
        "has parent crop group"@en ;
    rdfs:domain srppp:CropGroup ;
    rdfs:range srppp:CropGroup .

srppp:hasPermissionHolder a owl:ObjectProperty ;
    rdfs:label "hat Verkaufsbewilligungshalter"@de,
        "has permission holder"@en ;
    rdfs:comment "Diese Eigenschaft verknüpft ein Produkt mit der Firma, die berechtigt ist, es zu verkaufen."@de,
        "This property links a product to the company authorized to sell it."@en ;
    rdfs:domain srppp:Product ;
    rdfs:range <https://schema.org/Organization> ;
    owl:inverseOf srppp:holdsPermissionToSell .

srppp:hasUnit a owl:ObjectProperty ;
    rdfs:label "hat Einheit"@de,
        "has Unit"@en ;
    rdfs:domain srppp:TreatmentConstraint .

srppp:holdsPermissionToSell a owl:ObjectProperty ;
    rdfs:label "hat Bewilligung zum Verkauf von"@de,
        "holds Permission to sell"@en ;
    rdfs:domain <https://schema.org/Organization> ;
    rdfs:range srppp:Product .

srppp:interactsWith a owl:ObjectProperty ;
    rdfs:label "interagiert mit"@de,
        "interacts with"@en ;
    rdfs:comment "Allgemeine Beziehung für physische Dinge, die mit etwas anderem interagieren. Ein Tier *interagiert mit* seiner Nahrung. Ein Pestizid *interagiert mit* dem Zielorganismus."@de,
        "General relationship for physical thingns that interact with something else. A animal *interacts with* its food. A pesticide *interacts with* the target organism."@en .

srppp:involves a owl:ObjectProperty ;
    rdfs:label "involviert"@de,
        "involves"@en ;
    rdfs:domain srppp:Treatment ;
    rdfs:range srppp:Product ;
    owl:inverseOf srppp:isInvolvedIn .

srppp:isAffectedBy a owl:ObjectProperty ;
    rdfs:label "ist betroffen von"@de,
        "is affected by"@en ;
    rdfs:domain srppp:CropGroup ;
    rdfs:range srppp:Damage ;
    owl:inverseOf srppp:affects .

srppp:isCausedBy a owl:ObjectProperty ;
    rdfs:label "wird verursacht von"@de,
        "is caused by"@en ;
    rdfs:domain srppp:Damage ;
    rdfs:range srppp:CropStressor ;
    owl:inverseOf srppp:causes .

srppp:isChemicallyDefinedBy a owl:ObjectProperty ;
    rdfs:label "ist chemisch definiert durch"@de,
        "is chemically defined by"@en ;
    rdfs:comment "Diese Eigenschaft verknüpft ein chemisches Produkt mit genau einer Substanz. Sie ermöglicht es, das Produkt in all seine chemischen Bestandteile und die jeweiligen Prozentsätze zu zerlegen."@de,
        "This property links a chemical product to exactly one substance. It allows to decompose the product to all its chemical ingredients and the respective percentages."@en ;
    rdfs:domain srppp:ChemicalProduct ;
    rdfs:range srppp:Substance ;
    owl:inverseOf srppp:chemicallyDefines .

srppp:isConcernedBy a owl:ObjectProperty ;
    rdfs:label "ist betroffen von"@de,
        "is concerned by"@en ;
    rdfs:domain srppp:Action ;
    rdfs:range srppp:ActionNotice ;
    owl:inverseOf srppp:concerns .

srppp:isContainedIn a owl:ObjectProperty ;
    rdfs:label "ist beinhaltet in"@de,
        "is contained in"@en ;
    rdfs:domain srppp:PureSubstance ;
    rdfs:range srppp:ChemicalProduct ;
    owl:inverseOf srppp:contains .

srppp:isDamagedBy a owl:ObjectProperty ;
    rdfs:label "wird beschädigt durch"@de,
        "is damaged by"@en ;
    rdfs:domain srppp:CropGroup ;
    rdfs:range srppp:CropStressor ;
    rdfs:subPropertyOf srppp:interactsWith ;
    owl:inverseOf srppp:damages .

srppp:isDefinedByBiologicalTaxon a owl:ObjectProperty ;
    rdfs:label "ist definiert durch das biologische Taxon"@de,
        "is defined by biological taxon"@en ;
    rdfs:range wd:Q16521 .

srppp:isInvolvedIn a owl:ObjectProperty ;
    rdfs:label "ist involviert in"@de,
        "is involved in"@en ;
    rdfs:domain srppp:Product ;
    rdfs:range srppp:Treatment ;
    owl:inverseOf srppp:involves .

srppp:isLimitedBy a owl:ObjectProperty ;
    rdfs:label "wird eingeschränkt durch"@de,
        "is limited by"@en ;
    rdfs:domain srppp:Damage ;
    rdfs:range srppp:CropProtectionProduct ;
    owl:inverseOf srppp:limits .

srppp:isManagedBy a owl:ObjectProperty ;
    rdfs:label "wird bekämpft von"@de,
        "is managed by"@en ;
    rdfs:domain srppp:CropStressor ;
    rdfs:range srppp:CropProtectionProduct ;
    rdfs:subPropertyOf srppp:interactsWith ;
    owl:inverseOf srppp:manages .

srppp:isMetabolizedFrom a owl:ObjectProperty ;
    rdfs:label "wird metabolisiert aus"@de,
        "is metabolized from"@en ;
    rdfs:domain srppp:PureSubstance ;
    rdfs:range srppp:PureSubstance ;
    owl:inverseOf srppp:isMetabolizedTo .

srppp:isMetabolizedTo a owl:ObjectProperty ;
    rdfs:label "wird metabolisiert zu"@de,
        "is metabolized to"@en ;
    rdfs:domain srppp:PureSubstance ;
    rdfs:range srppp:PureSubstance .

srppp:isMitigatedBy a owl:ObjectProperty ;
    rdfs:label "wird vermindert durch"@de,
        "is mitigated by"@en ;
    rdfs:domain srppp:Damage ;
    rdfs:range srppp:Action ;
    owl:inverseOf srppp:mitigates .

srppp:isProtectedBy a owl:ObjectProperty ;
    rdfs:label "wird geschützt durch"@de,
        "is protected by"@en ;
    rdfs:domain srppp:CropGroup ;
    rdfs:range srppp:Action ;
    owl:inverseOf srppp:protects .

srppp:isSameProductAs a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "ist dasselbe Produkt wie"@de,
        "is the same product as"@en ;
    rdfs:comment "Gibt an, dass zwei Produkte in ihrer chemischen Zusammensetzung oder in ihrem Organismus identisch sind, aber unter verschiedenen Namen verkauft werden. Solche zwei Produkte haben auch die gleiche Basis der Bundeszulassungsnummer. Zum Beispiel haben die Produkte mit den Zulassungsnummern W-6761 und W-6761-1 die gleiche chemische Zusammensetzung."@de,
        "Indicates that two products are identical in their chemical composition or the contained organism but are sold under different names. Such two products also share the same federal admission number base. For example, the products with admission numbers W-6761 and W-6761-1 have the same chemical composition."@en ;
    rdfs:domain srppp:Product ;
    rdfs:range srppp:Product .

srppp:isSimilarProductAs a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "ist ein ähnliches Produkt wie"@de,
        "is a similar product as"@en ;
    rdfs:comment "Gibt an, dass zwei Produkte in ihrer Funktion ähnlich sind, obwohl sie in ihrer chemischen Zusammensetzung oder ihrem Organismus nicht identisch sind."@de,
        "Indicates that two products are similar in their function, although not identical in their chemical composition or their organism."@en ;
    rdfs:domain srppp:Product ;
    rdfs:range srppp:Product .

srppp:knows a owl:ObjectProperty ;
    rdfs:label "kennt"@de,
        "knows"@en ;
    rdfs:domain foaf:Person .

srppp:limits a owl:ObjectProperty ;
    rdfs:label "schränkt ein"@de,
        "limits"@en ;
    rdfs:comment "Diese Eigenschaft zeigt an, dass ein Pflanzenschutzmittel bestimmte Schadensarten reduziert oder mildert."@de,
        "This property indicates that a crop protection product reduces or mitigates specific types of damage."@en ;
    rdfs:domain srppp:CropProtectionProduct ;
    rdfs:range srppp:Damage .

srppp:manages a owl:ObjectProperty ;
    rdfs:label "bekämpft"@de,
        "manages"@en ;
    rdfs:comment "Diese Eigenschaft zeigt an, dass ein Pflanzenschutzmittel verwendet wird, um einen bestimmten Stressor zu bekämpfen."@de,
        "This property indicates that a crop protection product is used to control or combat a specific crop stressor."@en ;
    rdfs:domain srppp:CropProtectionProduct ;
    rdfs:range srppp:CropStressor ;
    rdfs:subPropertyOf srppp:interactsWith .

srppp:mitigates a owl:ObjectProperty ;
    rdfs:label "mindert"@de,
        "mitigates"@en ;
    rdfs:comment "Diese Eigenschaft zeigt an, dass eine Handlung bestimmte Schadensarten reduziert oder lindert."@de,
        "This property indicates that an action reduces or alleviates specific types of damage."@en ;
    rdfs:domain srppp:Action ;
    rdfs:range srppp:Damage .

srppp:protects a owl:ObjectProperty ;
    rdfs:label "schützt"@de,
        "protects"@en ;
    rdfs:domain srppp:Action ;
    rdfs:range srppp:CropGroup .

<https://schema.org/address> a owl:ObjectProperty ;
    rdfs:label "hat Adresse"@de,
        "has address"@en ;
    rdfs:domain <https://schema.org/Organization> ;
    rdfs:range <https://schema.org/PostalAddress> .

<https://schema.org/addressCountry> a owl:ObjectProperty ;
    rdfs:label "befindet sich in Land"@de,
        "is located in country"@en ;
    rdfs:domain <https://schema.org/PostalAddress> ;
    rdfs:range <https://schema.org/Country> .

srppp: a owl:Ontology ;
    dcterms:creator "Federal Office for Agriculture" ;
    dcterms:description "Diese Ontologie veranschaulicht den Wert einer umfassenden und gut strukturierten Ontologieentwicklung für das Bundesamt für Landwirtschaft (und allgemein den Schweizer Agrar- und Lebensmittelsektor). Durch die formale Modellierung der Komplexität des Pflanzenschutzes (mit Schwerpunkt auf zugelassenen Produkten) verbessert sie das Verständnis der aktuellen Regeln und ermöglicht deren effektive Kommunikation über IT-Dienste."@de,
        "This ontology exemplifies the value of comprehensive and well-structured ontology development for the Swiss Federal Office for Agriculture (and more broadly the Swiss agri-food sector). By capturing the complexities of crop protection (with a focus on registered products) in a formal model, it enhances understanding of current rules and allows their effective communication via IT-services."@en ;
    dcterms:title "Eine (demonstrative) Ontologie für den Pflanzenschutz in der Schweiz"@de,
        "A (demonstrative) ontology for crop protection in Switzerland"@en ;
    owl:versionInfo "0.11" .

srppp:Safener rdfs:label "Safener"@de,
        "Safener"@en ;
    rdfs:subClassOf srppp:Substance .

